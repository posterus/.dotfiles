Connection='wireless'
Interface=wlp3s0 
Security='wpa-configsection'
Description="eduroam network"
IP='dhcp'
TimeoutWPA=10
WPAConfigSection=(
    'ssid="eduroam"'
    'key_mgmt=WPA-EAP'
    'eap=TTLS'
    'proto=WPA2'
    'phase2="auth=PAP"'
    'anonymous_identity="ID@student.lu.se"' # ex: tu-dresden.de
    'identity="ID@student.lu.se"' # ex: s4234381@tu-dresden.de
    'ca_cert="/usr/share/ca-certificates/trust-source/mozilla.trust.crt"'
    'password="password"'
)
